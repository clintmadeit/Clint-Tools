template: default.qss
palette: default.yaml
variables:
  path: default.yaml
  overrides:
    combobox: |-
      qlineargradient(
        x1:0, y1:0, x2:0, y2:1,
        stop:0 #1f1f27, stop: 0.3 #23232b, stop: 0.6 #23232b, stop:1 #1f1f27
      )
system:
  path: default.yaml
  overrides:
    daw:
      seq_antialiasing: true
      seq_item_background: "#12666666"
      seq_item_background_use_track_color: true
    widgets: {}
